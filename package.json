  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@remix-run/dev": "^2.13.1",
    "@tailwindcss/vite": "4.0.0-alpha.28",
    "@types/bun": "^1.1.11",
    "@types/cli-table": "^0.3.4",
